<tr class="<%= cycle('table0', 'table1') -%>">
  <td><%= link_to(import.id.to_s, import_path(import.id)) %> </td>
  <td><%= link_to(( h import.import_type), import_path(import.id), :method => :get)  -%></td>
  <td><%= link_to(( h import.user.login), user_path(import.user.id), :method => :get) if import.user  -%></td>
  <td><%=h time_ago_in_words(import.created_at) -%> ago</td>
  <td>
    <% if import.finished_at %>
      <%=h time_ago_in_words(import.finished_at) -%> ago<% end %>
  </td>
  <td><%= import.status  %> </td>
</tr>
